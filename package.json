{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean",
    "dev": "next dev",
    "test": "turbo test",
    "jobs:dashboard": "turbo jobs --filter=@midday/dashboard",
    "format": "biome format --write ./app",
    "lint": "turbo lint && manypkg check",
    "check:types": "turbo typecheck"
  },
  "dependencies": {
    "@biomejs/biome": "1.6.1",
    "@manypkg/cli": "^0.21.3",
    "date-fns": "^3.6.0",
    "geist": "^1.2.2",
    "next": "14.1.3",
    "next-international": "^1.2.4",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "turbo": "^1.12.5",
    "typescript": "^5"
  }
}
